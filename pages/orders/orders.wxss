/* pages/order/orders.wxss */
.top-nav {
  width: 750rpx;
  position: fixed;
  top: 0;
  z-index: 1;
}
.top-nav-bar {
  display: flex;
  justify-content: space-around;
  font-size: 13px;
  padding: 10px 0;
  background-color: #fff;
  border-top: 1px solid #e5e5e5;
}
.top-nav-bar .active {
  /*border-bottom: 2px solid #09bb07;*/
  border-bottom: 2px solid #3a82f8;
}
.ma-top-120 {
  margin-top: 120rpx;
}